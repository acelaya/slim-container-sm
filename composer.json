{
    "name": "acelaya/slim-container-sm",
    "description": "A Slim framework container wrapping a ZF2 ServiceManager",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Celaya",
            "email": "alejandro@alejandrocelaya.com"
        }
    ],
    "keywords": [
        "slim",
        "zend framework",
        "zf2",
        "dic",
        "dependency injection"
    ],
    "require": {
        "php": "^5.5|^7.0",
        "zendframework/zend-servicemanager": "~3.0",
        "slim/slim": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Acelaya\\SlimContainerSm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acelaya\\SlimContainerSm\\Test\\": "tests/"
        }
    },
    "suggest": {
        "akrabat/rka-slim-controller": "Allows to create controllers as a service in Slim Framework",
        "slimcontroller/slimcontroller": "Allows to create controllers as a service in Slim Framework"
    }
}
